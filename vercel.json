{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/webhook", "dest": "/api/index.js" },
    { "src": "/w/(.*)", "dest": "/api/index.js" },
    { "src": "/c/(.*)", "dest": "/api/index.js" },
    { "src": "/location", "dest": "/api/index.js" },
    { "src": "/camsnap", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}